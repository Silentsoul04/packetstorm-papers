      /$cx $cx rs1$shdelta sub def
      /$cy $cy rs1$shdelta sub def
      }
      loop
    /$cx 0 def /$cy 0 def
{
/$cx $cx rs1$shdelta add def
      /$cy $cy rs1$shdelta add def
      $cx $brad gt { exit } if
      $cx $brad moveto
      $brad $cy lineto stroke
      }
