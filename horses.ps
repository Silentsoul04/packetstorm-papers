/rs1$rhatch
    {
    newpath
    /$cx 0 def /$cy $brad def
        {
/$cx $cx rs1$shdelta add def
/$cy $cy rs1$shdelta sub def
      $cy 0 lt { exit } if
0 $cy moveto
      $cx $brad lineto stroke
      }
      loop
    /$cx $cy abs def /$cy $brad $cy add def
{
$cx $brad gt { exit } if
      $cx 0 moveto
      $brad $cy lineto stroke
      /$cx $cx rs1$shdelta add def
      /$cy $cy rs1$shdelta sub def
      }
loop
    }
