%% % procedure   : rs1$lin
%% % purpose     : adds new point to lineseg arrays, calling lineseg_print
%% %               if new point is not on the same line as the previous point.
%% %               OR if the new segment is VERTICAL
%% % parameters  : <nx> <ny> <oldx> <oldy>
%% %               The 'backwards' format is for ease of calculations.
%% % side effects: Data gets added to the lineseg arrays.
%% %               Data may get printed out, clearing the lineseg arrays.
%% % return value: none
%% % Notes       :
%% %   Why print out if new segment is vertical?  So boxes around text
%% %   won't be smoothed!  Trust me, this looks REALLY gross!
/rs1$lin 
    {
    3 copy 2 copy
    ly eq exch lx eq and not 
    4 1 roll exch pop eq or %handle y=y0 
    { rs1$lineseg_print rs1$lineseg_add } 
    { pop pop }
    ifelse
    rs1$lineseg_add
    } def
 % procedure   :rs1$lineseg_add
%% % purpose     : Adds an X and Y value (already on the stack) to the
%% %               xlineseg and ylineseg arrays.
%% % parameters  : xvalue yvalue 
%% % side effects: ilineseg is bumped by one, lx and ly values are set
/rs1$lineseg_add
    {
    2 copy
    ilineseg 4990 ge
        {
        xlineseg ilineseg 1 sub get %Save old values on stack
        ylineseg ilineseg 1 sub get
        rs1$lineseg_print
        rs1$lineseg_add %Put saved values into array as the first point.
        }
    if
    ylineseg exch ilineseg exch put
    xlineseg exch ilineseg exch put
    /ilineseg ilineseg 1 add def
    /ly exch def /lx exch def
    } def
%% % procedure   : rs1$lineseg_print
%% % parameters  : none
%% % side effects: ilinesg is set to zero; curve (or kinky line) is stroked.
%% % return value: none
%% % notes
%% %   This routine should not be tampered with lightly!
%% %   Does some bounds checking, and, if the line contained in the lineseg
%% %     arrays is long enough, starts working.  It will either do a number
%% %     of calculations to produce a smooth curve, or will cut to the chase
%% %     and make a kinky curve.
%% %     The inputs to the RCURVETO function are strange.  A line is drawn
%% %     from the current point, curved acording to 'hints' contained in 
%% %     a pair of coordinates, and ends up at the point in the coordinates
%% %     on the top of the stack.  That is, the stack contains
%% %             dx1 dy1 dx2 dy2 dx3 dy3
%% %     x0 and y0 are defined to be the currentpoint; all other coordinates
%% %     are relative to x0 and y0 (by using relative coordinated I saved
%% %     some mul and div operations).
%% %
%% %     dx1 and dy1 are the delta-x from x0 and y0 to the previous point.
%% %     They are scaled down by rs1$sf (smooth factor); this is critical.
%% %     rs1$sf should be about .2; 0 means no smoothing, .5 means 'make is
%% %     choppy', and values greater than .5 mean 'make curliques'.
%% %
%% %     Ditto for dx2 and dy2, but measured from this segment to the next
%% %     segment to be drawn.
  /rs1$lineseg_print
    {
    ilineseg 0 ne
        {
        % Set the dash 
        % 0=unused 1=single 2=double 3=dot 4=dash 5=dotdash 6=dotdotdash
        [[] [] [] [3 7] [15 10] [3 7 15 10][3 7 3 7 15 10]] rs1$ltyp get 0 setdash
        newpath
        ilineseg 4 lt
            { % Array is too small to smooth
            xlineseg 0 get ylineseg 0 get moveto
            xlineseg 1 get ylineseg 1 get lineto
            ilineseg 2 gt { xlineseg 2 get ylineseg 2 get lineto } if
            ilineseg 3 gt { xlineseg 3 get ylineseg 3 get lineto } if
            }
            { % Smooth that sucker.
            /lx xlineseg 0 get def /ly ylineseg 0 get def lx ly moveto
            /cx xlineseg 1 get def /cy ylineseg 1 get def cx cy lineto
            /nx xlineseg 2 get def /ny ylineseg 2 get def
            /farpt 3 def
            ilineseg 3 sub
                {
                /fx xlineseg farpt get def /fy ylineseg farpt get def
                nx ny lineto
                /lx cx def /ly cy def 
                /cx nx def /cy ny def
                /nx fx def /ny fy def
                /farpt farpt 1 add def
                } repeat
            fx fy lineto %Last segment
            }
        ifelse % first proc for boundard, second for reality
        rs1$ltyp 2 eq   
        { 6 setlinewidth gsave stroke grestore 
          1 setlinewidth 1 setgray stroke 0 setgray
        }
        { stroke }
        ifelse
        [] 0 setdash 
        }
    if  % if there is a linesegment to be drawn at all
    /ilineseg 0 def
    } def
/rs1$ltyp 1 def %init to single line
/rs1$sf .2 def % Smoothing factor for presentation graphics
/xlineseg 5000 array def /ylineseg 5000 array def /ilineseg 0 def
