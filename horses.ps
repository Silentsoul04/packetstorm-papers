        {
        rs1$style findfont rs1$size scalefont setfont
        /rs1$newfont false def
        }
    if
    dup
    /rs1$ostr exch def
    stringwidth pop
    /rs1$swidth exch def
    % move origin to current point
    gsave
    currentpoint
    translate
    % rotate coordinate system if doing rotated text
    rs1$angle 0 ne { rs1$angle rotate }if
    % handle replace writing or highlighted text
    rs1$replace rs1$highlight or
        {
        newpath
        0 rs1$cbase neg moveto
        rs1$swidth 0 rlineto
        0 rs1$cheight rlineto
        rs1$swidth neg 0 rlineto
        0 rs1$cheight neg rlineto
        closepath
        rs1$highlight { .75 setgray } { 1 setgray } ifelse
        fill
        0 setgray
        }
    if % end if rs1$replace or rs1$highlight
    0 0 moveto
    % finally, output the text
    rs1$ostr rs1$show
    grestore
    rs1$angle 0 eq { rs1$swidth 0 rmoveto } { 0 rs1$swidth rmoveto } ifelse
    }
