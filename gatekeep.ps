	CharacterDefs /espace { 250 0 moveto } put

	CharacterDefs /baselineen
		{ 500 0 lineto stroke } put

	CharacterDefs /boxrule
		{ -20 -220 moveto
	 currentpoint transform round exch round exch itransform newpath moveto
			20 -220 lineto
			20 800 lineto		%820
			-20 800 lineto closepath	%820
		  fill
		} put

	CharacterDefs /square
		{ 80 0 rmoveto
	 currentpoint transform round exch round exch itransform newpath moveto
		  640 0 rlineto 0 640 rlineto -640 0 rlineto closepath stroke
		} put
	CharacterDefs /leftceiling 
		{ 201 -225 moveto
	 currentpoint transform round exch round exch itransform newpath moveto
			0 1001 rlineto
			238 0 rlineto
			0 -54 rlineto
			-184 0 rlineto
			0 -947 rlineto closepath
		  fill
		} put
	CharacterDefs /rightceiling
		{ 201 -225 moveto
	 currentpoint transform round exch round exch itransform newpath moveto
			0 947 rlineto
			-184 0 rlineto
			0 54 rlineto
			238 0 rlineto
			0 -1001 rlineto closepath
		  fill
		} put
	CharacterDefs /leftfloor
		{ 201 -225 moveto
	 currentpoint transform round exch round exch itransform newpath moveto
			0 1001 rlineto
			54 0 rlineto
			0 -947 rlineto
			184 0 rlineto
			0 -54 rlineto closepath
		  fill
		} put
	CharacterDefs /rightfloor
		{ 201 -225 moveto
	 currentpoint transform round exch round exch itransform newpath moveto
			54 0 rlineto
			0 1001 rlineto
			-54 0  rlineto
			0 -947 rlineto
			-184 0 rlineto
			0 -54 rlineto closepath
		  fill
		} put
	CharacterDefs /fillsquare
		{ 80 0 rmoveto
		  640 0 rlineto 0 640 rlineto -640 0 rlineto
		  closepath fill } put
	CharacterDefs /circle
		{ 500 360 rmoveto currentpoint
		  newpath 333 0 360 arc
		  50 setlinewidth stroke } put
	CharacterDefs /emptybullet
		{ 200 250 rmoveto currentpoint
		  newpath 200 0 360 arc stroke } put
	CharacterDefs /verticalrule
		{ 0 800 moveto 0 -770 rlineto stroke } put

	/BuildChar
		{ BuildCharDict begin
			/char exch def
			/fontdict exch def
			/charname fontdict /Encoding get
			 char get def
			/charproc fontdict /CharacterDefs get
			 charname get def
			/charwidth fontdict /Metrics get
			 charname get def
			charwidth 0
			-250 -250 1000 1000 setcachedevice
			2 setlinejoin 40 setlinewidth
			newpath 0 0 moveto gsave charproc grestore
		  end
		} def
end

