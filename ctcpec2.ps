		dp_h mul
		exch dup 3 1 roll exch
		patmat currentmatrix pop
		translate
		dp_pw dp_ph inv
		dp_mat dp_proc imagemask
		patmat setmatrix
	    } for
	    pop
	} for
    } bind def
